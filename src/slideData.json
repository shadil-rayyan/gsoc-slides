  },
  {
    "title": "Final Message",
    "blocks": [
      {
        "type": "bullet",
        "text": "Start early"
      },
      {
        "type": "bullet",
        "text": "Be consistent"
      },
      {
        "type": "bullet",
        "text": "Communicate clearly"
      },
      {
        "type": "bullet",
        "text": "Build evidence"
      },
      {
        "type": "bullet",
        "text": "GSoC rewards effort, not luck"
      }
    ]
  }
]
